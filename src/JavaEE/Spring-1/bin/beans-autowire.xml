<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="address" class="space.snowwolf.spring.beans.autowire.Address" p:city="Beijing"
		p:street="HuiLongGuan"></bean>

	<bean id="car" class="space.snowwolf.spring.beans.autowire.Car"
		p:brand="Audi" p:price="300000"></bean>

	<bean id="person" class="space.snowwolf.spring.beans.autowire.Person"
		p:name="Tom" autowire="byType"></bean>

</beans>
